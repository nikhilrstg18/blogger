{
  "name": "blogger",
  "version": "1.0.0",
  "description": "A constantly evolving and thoughtful architecture for creating static blogs with Gatsby",
  "keywords": [
    "gatsby",
    "graphql",
    "react",
    "blog"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nikhilrstg18/blogger.git"
  },
  "license": "MIT",
  "author": "Nikhil Rustagi <nikhilrstg18@gmail.com>",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rimraf .cache public",
    "deploy:gh": "gatsby build --prefix-paths && gh-pages -d public",
    "format": "npm run format:ts && npm run format:scss",
    "format:scss": "stylelint \"src/**/*.scss\" --fix",
    "format:ts": "eslint \"src\" --ext .tsx,.ts --fix && prettier --write .",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:commit": "commitlint --edit",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:staged": "lint-staged",
    "lint:ts": "eslint \"src\" --ext .tsx,.ts && prettier --check .",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "serve": "gatsby serve",
    "start": "npm run clean && gatsby develop",
    "test": "jest --config ./internal/testing/jest-config.ts",
    "test:coverage": "jest --coverage --config ./internal/testing/jest-config.ts",
    "test:watch": "jest --watch --config ./internal/testing/jest-config.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{tsx?}": [
      "npm run format:ts"
    ],
    "*.{(sc|sa|c)ss}": [
      "npm run format:scss"
    ]
  },
  "dependencies": {
    "@sentry/gatsby": "^7.23.0",
    "@sentry/tracing": "^7.23.0",
    "classnames": "^2.3.2",
    "disqus-react": "^1.1.5",
    "gatsby": "^5.2.0",
    "gatsby-link": "^5.2.0",
    "gatsby-plugin-catch-links": "^5.2.0",
    "gatsby-plugin-feed": "^5.2.0",
    "gatsby-plugin-google-gtag": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-offline": "^6.2.0",
    "gatsby-plugin-optimize-svgs": "^1.0.5",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-plugin-sass": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-sitemap": "^6.2.0",
    "gatsby-remark-autolink-headers": "^6.2.0",
    "gatsby-remark-copy-linked-files": "^6.2.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^7.2.0",
    "gatsby-remark-prismjs": "^7.2.0",
    "gatsby-remark-responsive-iframe": "^6.2.0",
    "gatsby-remark-smartypants": "^6.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-remark": "^6.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@alxshelepenok/eslint-config": "^1.0.133",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@jest/globals": "^29.3.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.3.21",
    "@swc/jest": "^0.2.23",
    "@types/gatsby-transformer-remark": "^2.9.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "browserslist": "^4.21.4",
    "codecov": "^3.8.3",
    "concurrently": "^7.6.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^13.0.4",
    "lost": "^9.0.1",
    "postcss": "^8.4.19",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.0",
    "prettier-plugin-packagejson": "^2.3.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "semantic-release": "^19.0.5",
    "source-map-support": "^0.5.21",
    "stylelint": "^14.15.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
